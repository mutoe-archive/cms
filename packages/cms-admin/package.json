{
  "name": "cms-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "eslint 'src/**/*.ts?(x)' --fix",
    "pont:sync": "rm -f src/services/api-lock.json && pont && eslint 'src/services/**/*.ts?(x)' --fix"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.1.1",
    "swr": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/dom": "^7.22.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.1.1",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "browserslist": "^4.14.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "classnames": "^2.2.6",
    "css-loader": "^4.2.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint-config-react-app": "^6.0.0-next.77",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-resolve": "^26.2.2",
    "jest-watch-typeahead": "^0.6.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "pont-engine": "^1.0.3",
    "@mutoe/pont-engine": "^1.0.5",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "react-dev-utils": "^10.2.1",
    "react-test-renderer": "^16.13.1",
    "resolve": "^1.17.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.3",
    "semver": "^7.3.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.0.0",
    "ts-pnp": "^1.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "resolutions": {
    "lodash": "^4.17.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": "eslint --fix"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": false
      }
    },
    "extends": [
      "standard",
      "react-app",
      "plugin:react/recommended",
      "plugin:import/errors",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "import/no-unresolved": "off",
      "import/named": "off",
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "overrides": [
      {
        "files": [
          "src/constants/contents.ts",
          "src/**/*.content.ts"
        ],
        "rules": {
          "@typescript-eslint/explicit-module-boundary-types": "off"
        }
      },
      {
        "files": [
          "src/services/**"
        ],
        "rules": {
          "@typescript-eslint/no-unused-vars": "off",
          "@typescript-eslint/explicit-module-boundary-types": "off"
        }
      }
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/constants/**",
      "!src/services/**",
      "!src/serviceWorker.ts",
      "!src/index.tsx",
      "!src/App.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "src/**/*.tsx": {
        "branches": -5,
        "lines": -10,
        "functions": -5,
        "statements": -5
      }
    },
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
